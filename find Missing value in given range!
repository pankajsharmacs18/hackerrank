#include<iostream>
using namespace std;
int main()
{
	int size=0;
	cout<<"Enter size of arrays :";
	cin>>size;
	int arr[size];
	cout<<"Enter value 1 to "<<size;
	for(int i=0;i<size;i++)
	{
	cin>>arr[i];
	
	}
	cout<<"Entered Arrays is ";
	for(int i=0;i<size;i++)
	{
	cout<<arr[i]<<" ";
	}
	int sum=0;
	for(int i=0;i<size;i++)
	{
		sum=sum+arr[i];
	}
	int func=(size*(size+1))/2;
	int missingVal=func-sum;
	cout<<"Missing value is "<<missingVal;
}
